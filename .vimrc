:runtime! archlinux.vim

:set shiftwidth=4
:set autoindent
:set smartindent
:set tabstop=4
:set clipboard=unnamedplus
:set number
:set mouse=a
:set omnifunc=javascriptcomplete#CompleteJS
:set hlsearch

" File find 
:set path+=**
:set wildmenu
:set wildignore+=**/node_modules/**
:set hidden

set encoding=UTF-8
